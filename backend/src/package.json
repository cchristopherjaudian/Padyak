    "node": "18"
  },
  "main": "./dist/index.js",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase": "^10.1.0",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.3.1",
    "http-status": "^1.6.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "twilio": "^4.15.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.2",
    "eslint": "^8.15.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^3.1.0",
    "ts-node": "^10.9.1"
  },
  "private": true
}
