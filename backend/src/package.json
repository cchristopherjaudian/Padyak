{
  "name": "padyak",
  "description": "Cloud Functions for Firebase",
  "author": "Calil Christopher Jaudian - Software Engineer",
  "scripts": {
    "build": "npx tsc -b",
    "lint": "eslint .",
    "watch": "npx tsc -w",
    "serve": "npm run build && firebase emulators:start --only functions,firestore",
